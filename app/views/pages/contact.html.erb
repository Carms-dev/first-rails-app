<h1>Meet the team</h1>

<ul>
    <% @members.each do |member| %>
        <li>
            <%= member.capitalize %>
            <%= link_to 'Linkedin', '#' %>
        </li>
    <% end %>
</ul>

<form action="/contact" method="get">
  <input type="text" 
         name="member" 
         value="<%= params[:member] %>"
         placeholder="Who are you looking for?">
  <input type="submit">
</form>
